
public class Shapes {
	public static void main(String[] args)
	{
		int width=20;
		int height = 10;
		Shapes shape = new Shapes();
		shape.rectangeDraw(width, height);
		//rectangleDraw(width,height);
		
	}
	void rectangeDraw(int width, int height)
	{
		for(int i = 0; i < height; i++)
		{
			for(int j = 0; j < width; j++)
			{
				System.out.print("*");
			}
			System.out.println();
		}
	}
}
